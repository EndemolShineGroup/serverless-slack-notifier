  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Endemol Shine Group Technology",
    "url": "https://github.com/EndemolShineGroup"
  },
  "homepage": "https://github.com/EndemolShineGroup/serverless-slack-notifier",
