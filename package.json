    "type": "git",
    "url": "https://github.com/EndemolShineGroup/serverless-slack-notifier.git"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "pretest": "rimraf coverage/",
    "prebuild": "rimraf dist/",
