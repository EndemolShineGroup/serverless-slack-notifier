    "serverless-webpack": "^5.1.1",
    "source-map-support": "^0.5.6",
    "ts-jest": "^23",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5",
    "tslint-config-prettier": "^1",
    "tslint-eslint-rules": "^5",
    "typedoc": "^0.13.0",
    "typescript": "^3",
    "uuid": "^3.3.2",
    "webpack": "^4.5.0"
